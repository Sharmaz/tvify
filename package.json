{
  "name": "tvify",
  "description": "web app de busqueda para tv shows",
  "version": "0.1.0",
  "dependencies": {
    "jquery": "^2.2.1",
    "nib": "^1.1.0",
    "serve": "^1.4.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0"
  },
  "scripts": {
    "build-js": "browserify -t [ babelify --preset [ es2015 ] ] index.js > app.js",
    "style": "stylus --use nib --watch src/client/lib/style.styl --out public/style.css",
    "jade": "jade --watch --pretty src/client/lib/index.jade --out public",
    "serve": "serve public"
  }
}
